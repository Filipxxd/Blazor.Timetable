@typeparam TEvent where TEvent : class

<Dropdown Model=this
          Label=@Localizer["ImportType"]
          Selector="x => x.Type"
          Options=ImportTypes />

<Button Title=@Localizer["ButtonSave"]
        OnClick=SubmitAsync />

<Button Title=@Localizer["ButtonCancel"]
    OnClick=ModalService.Close 
    ButtonType=ButtonType.Cancel />
